{
  "scriptFile": "../function.jar",
  "entryPoint": "azure.tika.GroovyFunctions.groovyTika",
  "bindings": [
    {
      "type": "httpTrigger",
      "name": "content",
      "direction": "in",
      "dataType": "binary",
      "path": "container/myfile.bin",
      "authLevel": "function",
      "methods": [ "post" ]
    },
    {
      "type": "http",
      "name": "$return",
      "direction": "out"
    }
  ]
}
